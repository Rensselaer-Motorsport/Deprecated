                          {
                              USBUART_1_CyBtldrCommReset();
                          }
                      }
                  }
                  
                  timeoutMs = ((uint16) 10u * timeOut); /* Re-arm timeout */
                  
                  /* Wait on next packet */
                  while((USBUART_1_GetEPState(USBUART_1_BTLDR_OUT_EP) != USBUART_1_OUT_BUFFER_FULL) && \
                        (0u != timeoutMs))
                  {
                      CyDelay(USBUART_1_BTLDR_WAIT_1_MS);
                      timeoutMs--;
                  }
              
